    <div class="hero-body">
        <div class="container has-text-centered">
            <h1 class="title">
                <span class="icon is-medium iconalign">
                    <i class="fad fa-browser fa-1x" aria-hidden="true"></i>
                </span>
                <br />
                <span>Rovel Discord List</span>
            </h1>
            <h2 class="subtitle">
                The Ultimate Discord List with Emojis, Users, Servers & Bots!
            </h2>
        </div>
    </div>
</section>
<div class="container is-fluid">
 <div class="columns is-multiline is-centered">
  <span class="http-method has-background-primary">New Bots!</span>
  <%- bots.map(bot => include('partials/card', {bot})).join(""); %>
  </div>
</div>
 <%- include('partials/footer'); %>
</body>
</html>