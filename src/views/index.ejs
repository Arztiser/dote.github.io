<%- include('partials/header', {
title: "Rovel Discord List - RDL",
desc: "The next gen all-in-one Discord List for Emojis, Users, Servers and Bots! Find the best stuff for yourself here! Or reach the top of the leaderboard with your friends!",
img: "/assets/img/bot/logo-512.png",
theme
}); %>
<script type="application/ld+json">
{
    "@context": "http://schema.org",
    "@type": "BreadcrumbList",
    "itemListElement": [{
        "@type": "ListItem",
        "position": 1,
        "name": "Home",
        "item": "https://discord.rovelstars.com"
    }, {
        "@type": "ListItem",
        "position": 2,
        "name": "Emojis",
        "item": "https://discord.rovelstars.com/emojis"
    }, {
        "@type": "ListItem",
        "position": 3,
        "name": "Users",
        "item": "https://discord.rovelstars.com/users"
    },
    {
        "@type": "ListItem",
        "position": 4,
        "name": "Servers",
        "item": "https://discord.rovelstars.com/servers"
    },
    {
        "@type": "ListItem",
        "position": 5,
        "name": "Bots",
        "item": "https://discord.rovelstars.com/bots"
    },{
        "@type": "ListItem",
        "position": 6,
        "name": "Dashboard",
        "item": "https://discord.rovelstars.com/dashboard"
    }]
}
</script>
</head>
<body>
 <%- include('partials/load'); %>
 <%- include('partials/navbar', {user}); %>
 <section class="hero">
    <div class="hero-body">
        <div class="container has-text-centered">
            <h1 class="title">
             <img src="/assets/img/dlogo.svg" />
                <br /><br />
                <span><%= __("title") %></span>
            </h1>
            <h2 class="subtitle">
                <%= __("desc") %>
            </h2>
        </div>
    </div>
</section>
<div class="container is-fluid">
            <div class="section">
                <h2 class="is-size-3 has-text-centered has-text-info">
                    <span class="icon is-medium iconalign">
                        <i class="bx bxs-bot"></i>
                    </span>
                    <span><%= __("title.bots.random") %></span>
                </h2>
                <br />
 <div class="columns is-multiline is-centered">
  <%- bots.map(bot => include('partials/card', {bot})).join(""); %>
  </div>
</div>
</div>
 <%- include('partials/footer'); %>
</body>
</html>