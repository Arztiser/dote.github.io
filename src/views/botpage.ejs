<%- include('partials/header', {
title: `${bot.tag} - Rovel Discord List`,
desc: `${bot.short}`,
img: bot.avatarURL
}); %>
<script src="https://unpkg.com/wc-discord-message@^2.0.0/dist/wc-discord-message/wc-discord-message.js"></script>
<script type="application/ld+json">
{
    "@context": "http://schema.org",
    "@type": "BreadcrumbList",
    "itemListElement": [{
        "@type": "ListItem",
        "position": 1,
        "name": "View Bot",
        "item": "https://discord.rovelstars.com/bots/<%= bot.id %>"
    }, {
        "@type": "ListItem",
        "position": 2,
        "name": "Bot",
        "item": "<%= bot.invite %>"
    }]
}
</script>
</head>
<body>
 <%- include('partials/load'); %>
 <%- include('partials/navbar', {user}); %>
 <section class="hero" style="background-image: url(<%= bot.bg %>); background-size: cover;">
  <div class="hero-body">
   <div class="container">
    <div class="columns">
     <div class="column is-2">
      <img class="avatar image is-128x128" style="margin: auto; border-radius: 50%;" src="<%= bot.avatarURL %>" alt="<%= bot.tag %> Avatar">
     </div>
     <div class="column is-10">
         <h1 class="title"><%= bot.username %>
   </h1><br />
   <h4 class="subtitle">
    <%= bot.short %>
   </h4>
   <div class="field is-grouped is-grouped-multiline">
   <div class="control">
    <div class="tags has-addons">
    <span class="tag is-dark">Prefix</span>
    <span class="tag is-danger"><%= bot.prefix %></span>
    </div>
  </div><br />
   <div class="control">
    <div class="tags has-addons">
    <span class="tag is-dark">Library</span>
    <% if(bot.lib == ""){ %>
    <span class="tag is-info">not known</span>
    <% } else { %>
    <span class="tag is-info"><%= bot.lib %></span>
    <% } %>
    </div>
  </div><br />
  <div class="control">
    <div class="tags has-addons">
    <span class="tag is-dark">Made by</span>
    <span class="tag is-success"><%= bot.owner.join(", ") %></span>
    </div>
  </div><br />
  <div class="control">
    <div class="tags has-addons">
    <span class="tag is-dark">In</span>
    <span class="tag is-link"><%= (bot.servers)?bot.servers:"???" %> Servers
    </span>
    </div>
  </div><br />
  <div class="control">
    <div class="tags has-addons">
    <span class="tag is-dark">Votes</span>
    <span class="tag is-info"><%= bot.votes %></span>
    </div>
  </div><br /><% if(bot.support){ %>
  <div class="control">
    <div class="tags has-addons">
     <a href="https://discord.gg/<%= bot.support %>">
    <span class="tag is-dark" style="color:white;">Support Server</span><span class="tag is-primary" style="color:white;">Join</span></a>
    </div>
    </div><br /><% } %>
    <% if(bot.github){ %>
    <div class="control">
    <div class="tags has-addons">
     <a href="<%= bot.github %>">
    <span class="tag is-dark" style="color:white;">Github</span><span class="tag is-success" style="color:black;">Check Source Code</span></a>
    </div>
  </div>
    <% } %>
    <% if(bot.donate){ %>
    <div class="control">
    <div class="tags has-addons">
     <a href="<%= bot.donate %>">
    <span class="tag is-dark" style="color:white;">Donate</span><span class="tag is-warning" style="color:black;">Money</span></a>
    </div>
  </div>
    <% } %>
  </div>
  </div>
  </div>
 </section>
 <br />
 <div class="container" style="overflow: hidden;">
  <div class="section content">
   <% if(bot.card.title){ %>
   <div class="column is-one-half is-announcement">
<div class="box has-background-dark" style="overflow: hidden; height: 100%;">
<article class="media">
<div class="media-left">
<figure class="image is-64x64">
<img width="64" height="64" class="is-rounded" loading="lazy" src="<%= bot.card.img %>" alt="<%= bot.card.title %>" onerror="this.onerror=null;this.src='/assets/img/logo.png';">
</figure>
</div>
<div style="overflow: hidden;" class="media-content">
<div class="content">
<p>
<span class="is-size-4 title has-text-default is-mobile" style="font-family: discord;">
<span><%= bot.card.title %></span>
</span>
<br />
<span class="has-text-grey-light"><%= bot.card.msg %></span>
</p>
</div>
</div>
</article>
</div>
</div>
<% } %>
   <%- bot.desc %>
   <br />
   <br />
    <div id="disqus_thread"></div>
    <br />
    <div id="disqus_recommendations"></div>
  </div>
 </div>
<script>
    /**
    *  RECOMMENDED CONFIGURATION VARIABLES: EDIT AND UNCOMMENT THE SECTION BELOW TO INSERT DYNAMIC VALUES FROM YOUR PLATFORM OR CMS.
    *  LEARN WHY DEFINING THESE VARIABLES IS IMPORTANT: https://disqus.com/admin/universalcode/#configuration-variables    */
    var disqus_config = function () {
    this.page.url = "https://discord.rovelstars.com/bots/<%= bot.id %>";  // Replace PAGE_URL with your page's canonical URL variable
    this.page.identifier = "<%= bot.id %>"; // Replace PAGE_IDENTIFIER with your page's unique identifier variable
    };
    (function() { // DON'T EDIT BELOW THIS LINE
    var d = document, s = d.createElement('script');
    s.src = 'https://roveldiscord.disqus.com/embed.js';
    s.setAttribute('data-timestamp', +new Date());
    (d.head || d.body).appendChild(s);
    window.DISQUS_RECOMMENDATIONS.reset()
    })();
</script>
<noscript>Please enable JavaScript to view the <a href="https://disqus.com/?ref_noscript">comments.</a></noscript>
      <%- include('partials/footer'); %>
</body>
</html>