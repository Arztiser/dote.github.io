<%- include('partials/header', {
title: `${bot.tag} - Rovel Discord List`,
desc: `${bot.short}`,
img: bot.avatarURL
}); %>
</head>
<body>
 <%- include('partials/load'); %>
 <%- include('partials/navbar', {user}); %>
 <section class="hero is-black is-medium">
  <div class="hero-body">
   <span class="title">
    <img class="image is-64x64 is-rounded" src="<%= bot.avatarURL %>" alt="<%= bot.tag %> Avatar"> <%= bot.username %>
   </span>
   <span class="subtitle">
    <%= bot.short %>
   </span>
  </div>
 </section>
 <div class="content is-fluid">
  <%- bot.desc %>
 </div>
      <%- include('partials/footer'); %>
</body>
</html>